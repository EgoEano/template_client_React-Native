{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@client/*": [
        "client/*"
      ],
      "@client/core/*": [
        "client/core/*"
      ],
      "@client/services/*": [
        "client/core/services/*"
      ],
      "@client/ui/*": [
        "client/core/ui/*"
      ],
      "@client/modules/*": [
        "client/modules/*"
      ],
      "@client/app/*": [
        "client/app/*"
      ],
      "@public/*": [
        "public/*"
      ],
      "@assets/*": [
        "public/assets/*"
      ],
      "@types/*": [
        "client/core/types/*"
      ]
    }
  }
}